{
  "schemaVersion": 1,
  "id": "music-addon",
  "version": "${version}",
  "name": "Meteor Music",
  "description": "A Music addon to Meteor client.",
  "authors": [
    "devmello"
  ],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/music/icon.png",
  "environment": "client",
